      "name": "Compare Countries",
      "disabled": false,
      "guid": "0c82c9aee2b3bbb65b6197ff2ddfb9d6",
      "options": {
        "rules": [
          {
            "type": "regex",
            "value": "{{ .lookup_country.body.nativeName }}",
