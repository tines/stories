terraform {
    required_providers {
        tines = {
        source = "github.com/tuckner/tines"
        version = ">=0.0.13"
        }
    }
}

provider "tines" {
    email    = var.tines_email
    base_url = var.tines_base_url
    token    = var.tines_token
}

resource "tines_story" "crowdstrike_response" {
    name = "CrowdStrike Response"
    team_id = var.team_id
    description = <<EOF
Story to alert on, and respond to CrowdStrike alerts and events.

By default, this story will send an email containing details of each alert. The recipient can initiate response actions, such as containing the device in Crowdstrike, by clicking links in the email body.

Template formatting for Jira and The Hive incidents can be found in the 'Build Detection' agent. These can be used to quickly configure alert creation in either of those platforms.
EOF
}


resource "tines_global_resource" "slack_webhook_url" {
    name = "slack_webhook_url"
    value_type = "text"
    value = "replaceme"
}

resource "tines_global_resource" "jira_username" {
    name = "jira_username"
    value_type = "text"
    value = "replaceme"
}

resource "tines_global_resource" "crowdstrike_domain" {
    name = "crowdstrike_domain"
    value_type = "text"
    value = "replaceme"
}

resource "tines_global_resource" "jira_domain" {
    name = "jira_domain"
    value_type = "text"
    value = "replaceme"
}

resource "tines_agent" "extract_chrome_extensions_prompt_0" {
    name = "Extract Chrome Extensions Prompt"
    agent_type = "Agents::TriggerAgent"
    story_id = tines_story.crowdstrike_response.id
    keep_events_for = 0
    source_ids = []
    receiver_ids = [tines_agent.extract_chrome_extensions_7.id]
    position = {
      x = 600.0
      y = -915.0
    }
    agent_options = jsonencode({"rules": [{"path": "{{.send_security_email.prompt.status}}", "type": "field==value", "value": "chrome"}]})
}

resource "tines_agent" "explode_individual_detections_1" {
    name = "Explode Individual Detections"
    agent_type = "Agents::EventTransformationAgent"
    story_id = tines_story.crowdstrike_response.id
    keep_events_for = 0
    source_ids = []
    receiver_ids = [tines_agent.trigger_if_overwatch_5.id, tines_agent.build_detection_13.id, tines_agent.create_new_issue_types_in_jira_17.id]
    position = {
      x = 600.0
      y = -1410.0
    }
    agent_options = jsonencode({"mode": "explode", "path": "{{.get_detection_details.body.resources}}", "to": "individual_detection"})
}

resource "tines_agent" "contain_device_2" {
    name = "Contain Device"
    agent_type = "Agents::SendToStoryAgent"
    story_id = tines_story.crowdstrike_response.id
    keep_events_for = 0
    source_ids = []
    receiver_ids = []
    position = {
      x = 840.0
      y = -1245.0
    }
    agent_options = jsonencode({"payload": {"host_id": "{{.explode_individual_detections.individual_detection.device.device_id}}"}, "story": "{% story Crowdstrike - Contain Device %}"})
}

resource "tines_agent" "search_for_host_group_members_in_crowdstrike_falcon_3" {
    name = "Search for Host Group Members in Crowdstrike Falcon"
    agent_type = "Agents::HTTPRequestAgent"
    story_id = tines_story.crowdstrike_response.id
    keep_events_for = 0
    source_ids = []
    receiver_ids = []
    position = {
      x = 2280.0
      y = -1515.0
    }
    agent_options = jsonencode({"content_type": "json", "headers": {"Authorization": "Bearer {% credential crowdstrike-oauth2 %}"}, "log_error_on_status": [], "method": "get", "payload": {}, "url": "https://api.crowdstrike.com/intel/combined/actors/v1"})
}

resource "tines_agent" "explode_crowdstrike_behaviors_4" {
    name = "Explode Crowdstrike Behaviors"
    agent_type = "Agents::EventTransformationAgent"
    story_id = tines_story.crowdstrike_response.id
    keep_events_for = 0
    source_ids = []
    receiver_ids = [tines_agent.deduplicate_events_8.id]
    position = {
      x = 600.0
      y = -1080.0
    }
    agent_options = jsonencode({"mode": "explode", "path": "{{.explode_individual_detections.individual_detection.behaviors}}", "to": "individual_behavior"})
}

resource "tines_agent" "trigger_if_overwatch_5" {
    name = "Trigger if Overwatch"
    agent_type = "Agents::TriggerAgent"
    story_id = tines_story.crowdstrike_response.id
    keep_events_for = 0
    source_ids = []
    receiver_ids = [tines_agent.contain_device_2.id]
    position = {
      x = 840.0
      y = -1320.0
    }
    agent_options = jsonencode({"rules": [{"path": "{{.explode_individual_detections.individual_detection.behaviors[0].tactic}}", "type": "regex", "value": "Falcon Overwatch"}]})
}

resource "tines_agent" "get_detection_details_6" {
    name = "Get Detection Details"
    agent_type = "Agents::HTTPRequestAgent"
    story_id = tines_story.crowdstrike_response.id
    keep_events_for = 0
    source_ids = []
    receiver_ids = [tines_agent.explode_individual_detections_1.id]
    position = {
      x = 600.0
      y = -1485.0
    }
    agent_options = jsonencode({"content_type": "json", "headers": {"Authorization": "Bearer {{.CREDENTIAL.crowdstrike}}"}, "log_error_on_status": [], "manual_time": "60", "method": "post", "payload": {"ids": "{{.get_detections_in_crowdstrike_falcon.body.resources | as_object}}"}, "url": "https://api.{{.RESOURCE.crowdstrike_domain}}/detects/entities/summaries/GET/v1"})
}

resource "tines_agent" "extract_chrome_extensions_7" {
    name = "Extract Chrome Extensions"
    agent_type = "Agents::SendToStoryAgent"
    story_id = tines_story.crowdstrike_response.id
    keep_events_for = 0
    source_ids = []
    receiver_ids = [tines_agent.parse_extensions_12.id]
    position = {
      x = 600.0
      y = -825.0
    }
    agent_options = jsonencode({"payload": {"command": "runscript -CloudFile=\"tines_extract_chrome_extensions_from_endpoint\" -CommandLine=\"C:\"", "device_id": "{{.explode_crowdstrike_behaviors.individual_behavior.device_id}}"}, "story": "{% story Crowdstrike - Run RTR Command %}"})
}

resource "tines_agent" "deduplicate_events_8" {
    name = "Deduplicate Events"
    agent_type = "Agents::EventTransformationAgent"
    story_id = tines_story.crowdstrike_response.id
    keep_events_for = 0
    source_ids = []
    receiver_ids = [tines_agent.extract_chrome_extensions_prompt_0.id, tines_agent.contain_device_prompt_16.id]
    position = {
      x = 600.0
      y = -990.0
    }
    agent_options = jsonencode({"lookback": "100", "mode": "deduplicate", "path": "{{.explode_individual_detections.individual_detection.detection_id}}{{.send_security_email.prompt.status}}"})
}

resource "tines_agent" "get_detections_in_crowdstrike_falcon_9" {
    name = "Get Detections in Crowdstrike Falcon"
    agent_type = "Agents::HTTPRequestAgent"
    story_id = tines_story.crowdstrike_response.id
    keep_events_for = 0
    source_ids = []
    receiver_ids = [tines_agent.get_detection_details_6.id, tines_agent.update_detection_to_in_progress_11.id]
    position = {
      x = 600.0
      y = -1575.0
    }
    agent_options = jsonencode({"content_type": "json", "headers": {"Authorization": "Bearer {{.CREDENTIAL.crowdstrike}}"}, "log_error_on_status": [], "manual_time": "90", "method": "get", "payload": {"filter": "status:\u0027new\u0027", "limit": "10", "sort": "first_behavior|desc"}, "url": "https://api.{{.RESOURCE.crowdstrike_domain}}/detects/queries/detects/v1"})
}

